        </td>
        <td>
            @Html.DisplayFor(modelItem => item.grupoAlumno)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.alumnoID }) |
            @Html.ActionLink("Details", "Details", new { id=item.alumnoID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.alumnoID })
        </td>
